{
  "name": "@steedos-widgets/steedos-lib",
  "private": false,
  "version": "1.0.0-beta.31",
  "unpkg": "./dist/index.umd.js",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "http": "npx http-server ../ --port 8080 --cors -c -1",
    "build": "yarn build-types && yarn build-rollup",
    "build-types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build-rollup": "rollup -c rollup.config.ts && yarn build-uglifyjs",
    "build-uglifyjs": "yarn uglifyjs dist/index.umd.js -o dist/index.umd.min.js"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "gitHead": "c41242d76a6b4b7f51001d55d71c93ea674fcde2"
}
