{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "apps/*",
    "packages/**"
  ],
  "command": {
    "version": {
      "exact": true,
      "gitTagCommand": "git tag %s-lerna -m %s"
    },
    "publish": {
      "npmClient": "npm",
      "allowBranch": [
        "1.0",
        "1.1",
        "1.2"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  },
  "version": "1.1.4-beta.1"
}
